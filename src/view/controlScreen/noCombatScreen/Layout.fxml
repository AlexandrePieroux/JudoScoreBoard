<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<?import java.net.URL?>
<GridPane fx:id="container" fx:controller="view.controlScreen.noCombatScreen.NoCombatScreenController" xmlns:fx="http://javafx.com/fxml" id="combatForm" alignment="CENTER" hgap="10" vgap="10">
    <padding><Insets top="25" right="25" bottom="10" left="25" /></padding>
    <HBox spacing="30" alignment="CENTER" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="4" >
        <Text text="%NOCOMBAT_msg" id="msg"/>
    </HBox>

    <stylesheets>
        <URL value="@Style.css" />
    </stylesheets>
</GridPane>